<odoo>
    <menuitem id="menu_analytic_manager" name="Analytic Manager" sequence="10"/>
    <menuitem id="menu_analytic_result" name="Résultats Analytiques" parent="menu_analytic_manager" sequence="20"
              action="action_analytic_result"/>

     <record id="action_analytic_result" model="ir.actions.act_window">
        <field name="name">Résultats Analytiques</field>
        <field name="res_model">analytic.result</field>
